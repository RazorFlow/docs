<% for(var i=0; i<events.length; i++){ %>

<% var item = events[i] %>
<div id="<%= item.name %>" class='rfApiEvent'>
    
    <h3><a href="#<%= item.name %>" class="rfDocPermalink pull-right">#</a><%= item.name %></h3>
    <p>
        <code><%= item.name %>(<%= item.callback.callback.name %>)</code>
    </p>
    <p><%= item.desc %></p>
    <h4>Parameters</h4>
    <% if(item.callback.params) { %>
        <%- render('params.ejs', {tree: tree, params: item.callback.params}) %>
    <% } %>
</div>

<% } %>
