<div id="<%= item.method.name %>" class='rfApiMethod'>
    
    <h3><a class="rfPermalink" href="#<%= item.method.name %>">Permalink</a><%= item.method.name %></h3>
    <hr/>
    <p>
        <code><%= item.signature %></code>
    </p>
    <p><%= item.desc.content %></p>
    <h5>Params</h5>
    <% if(item.params.length && item.params[0]) { %>
        <%- render('params.ejs', {tree: tree, params: item.params, item_options_id: "options_" + item.method.name}) %>
    <% } else { %>
        None
    <% } %>
    <% if(item.example) { %>
        <h5>Example</h5>
        <pre><code><%= item.example.code %></code></pre>
    <% } %>
</div>

    

