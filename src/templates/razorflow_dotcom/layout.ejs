<%= currentClass %>
<div class="container">
    <div class="row">
        <div class="col-md-4">
            <div class="list-group api_nav_list">
                <%- render('nav.ejs', {items: tree.findAllClassNames(), currentClass: currentClass}) %>
            </div>
        </div>
        <div class="col-md-8">
            <% if(currentClass != 'index') { %>
                <%- render('class.ejs', {tree: tree, item: tree.findNodeByClassName(currentClass)}) %>
            <% } else { %>
                <%- render('index.ejs', {}) %>
            <% } %>
        </div>
    </div>
</div>

